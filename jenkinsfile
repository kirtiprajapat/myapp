pipeline {
    agent any 

    triggers {
        githubPush()
    }

    stages{
        stage('clone repo '){
            steps {
               git  'https://github.com/kirtiprajapat/myapp.git'
            }
        }
        stage("insatll dependencies"){
            steps {
                bat 'npm install'
            }
        }
        stage('run app'){
            steps{
                bat 'npm start'
            }
        }
    }
}