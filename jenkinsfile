pipeline {
    agents any 

    triggers {
        githubPush()
    }

    stages{
        stage('clone repo '){
            steps {
               git  'https://github.com/kirtiprajapat/myapp.git'
            }
        }
        stage("insatll dependencies"){
            steps {
                sh 'npm install'
            }
        }
        stage('run app'){
            steps{
                sh 'npm start'
            }
        }
    }
}